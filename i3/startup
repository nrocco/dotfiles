# Detect displays
/usr/bin/xprofile activate

# Set the background, do this before urxvtd starts
/usr/bin/bash /home/nrocco/.fehbg

# Start urxvtd daemon
/usr/bin/urxvtd -q -f -o &

# Start clipboard manager
/usr/bin/clipit &

# Start owncloud
/usr/bin/owncloud &

# Start redshift
/usr/bin/redshift &

# Start screen locker
/usr/bin/xss-lock -- /usr/bin/i3lock -t -n -i /home/nrocco/.dotfiles/backgrounds/ios-linen.png
